{
  "compilerOptions": {
    "target": "es2021",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "module": "commonjs" ,
    "paths": {} ,
    "resolveJsonModule": true ,
    "allowSyntheticDefaultImports": true ,
    "esModuleInterop": true ,
    "forceConsistentCasingInFileNames": true ,
    "strict": false ,
    "skipLibCheck": true ,
    "sourceMap": true,
    "lib": ["esnext"]
  }
}
